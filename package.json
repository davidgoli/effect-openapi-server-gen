{
  "name": "effect-openapi-server-gen",
  "version": "0.1.0",
  "description": "Generate Effect HttpApi server code from OpenAPI specifications",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "effect-openapi-server-gen": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "dev": "tsup --watch"
  },
  "keywords": [
    "effect",
    "openapi",
    "codegen",
    "typescript",
    "http",
    "server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@effect/cli": "^0.69.2",
    "@effect/platform": "^0.90.10",
    "@effect/platform-node": "^0.96.1",
    "effect": "^3.17.13",
    "js-yaml": "^4.1.0",
    "openapi-types": "^12.1.3",
    "swagger-parser": "^10.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^9.12.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.13.1"
}
